source=SELECT \n    e.PIN,\n    e.PNO,\n    e.EMPNAME,\n    e.DESIG,\n    e.JOBPOSITION,\n    e.EDUCATION,\n    e.EXPERIENCE,\n    e.DIVISION,\n    e.SECTION,\n    e.Project,\n    e.Role,\n    jp.JobPos_Descrip,\n    jp.JobPos_Purpose,\n    jp.JobPos_EduReq,\n    jp.JobPos_ExpReq,\n    jp.JobPos_Tools_Software,\n    jpc.job_areas,\n    jpc.general_tasks,\n    jpc.technical_tasks,\n    jpc.required_trainings,\n    GROUP_CONCAT(DISTINCT COALESCE(cat_acq.TrgDesc, eta.Acquired_Training) SEPARATOR \'; \') as acquired_trainings,\n    GROUP_CONCAT(DISTINCT COALESCE(cat_miss.TrgDesc, emt.Missing_Training) SEPARATOR \'; \') as missing_trainings\nFROM cat_employee e\nLEFT JOIN cat_jobposition jp ON e.JOBPOSITION = jp.JobPosition\nLEFT JOIN job_position_comprehensive jpc ON e.JOBPOSITION = jpc.JobPosition\nLEFT JOIN employee_trainings_acquired eta ON e.PIN = eta.PIN\nLEFT JOIN cat_alltrainings cat_acq ON eta.Acquired_Training = cat_acq.TrgCode\nLEFT JOIN employee_missing_trainings emt ON e.PIN = emt.PIN\nLEFT JOIN cat_alltrainings cat_miss ON emt.Missing_Training = cat_miss.TrgCode\nGROUP BY e.PIN, e.PNO, e.EMPNAME, e.DESIG, e.JOBPOSITION, e.EDUCATION, e.EXPERIENCE, \n         e.DIVISION, e.SECTION, e.Project, e.Role, jp.JobPos_Descrip, jp.JobPos_Purpose, \n         jp.JobPos_EduReq, jp.JobPos_ExpReq, jp.JobPos_Tools_Software, jpc.job_areas, \n         jpc.general_tasks, jpc.technical_tasks, jpc.required_trainings
