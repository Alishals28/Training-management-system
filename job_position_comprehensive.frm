source=SELECT \n    jp.JobPosition,\n    jp.JobPos_Descrip,\n    jp.JobPos_Purpose,\n    jp.JobPos_EduReq,\n    jp.JobPos_ExpReq,\n    jp.JobPos_Tools_Software,\n    GROUP_CONCAT(DISTINCT ja.JobArea SEPARATOR \'; \') as job_areas,\n    GROUP_CONCAT(DISTINCT jgt.JobGTask SEPARATOR \'; \') as general_tasks,\n    GROUP_CONCAT(DISTINCT jtt.JobTTask SEPARATOR \'; \') as technical_tasks,\n    GROUP_CONCAT(DISTINCT trj.TrgDesc SEPARATOR \'; \') as required_trainings\nFROM cat_jobposition jp\nLEFT JOIN x_jobposition_jobarea xja ON jp.JobPosition = xja.JobPosition\nLEFT JOIN cat_jobarea ja ON xja.JobAreaCode = ja.JobAreaCode\nLEFT JOIN x_jobposition_jobgtask xjg ON jp.JobPosition = xjg.JobPosition\nLEFT JOIN cat_jobgtask jgt ON xjg.JobGTaskCode = jgt.JobGTaskCode\nLEFT JOIN x_jobposition_jobttask xjt ON jp.JobPosition = xjt.JobPosition\nLEFT JOIN cat_jobttask jtt ON xjt.JobTTask = jtt.JobTTaskCode\nLEFT JOIN training_required_by_jobposition trj ON jp.JobPosition = trj.JobPosition\nGROUP BY jp.JobPosition, jp.JobPos_Descrip, jp.JobPos_Purpose, jp.JobPos_EduReq, jp.JobPos_ExpReq, jp.JobPos_Tools_Software
